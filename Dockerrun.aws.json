{
    "AWSEBDockerrunVersion": "1",
    "Image": {
        "Name": "khonokoo",
        "Update": "true"
    },
    "Ports": [
        {
            "ContainerPort": "8081",
            "HostPort": "80"
        }
    ],
    "Volumes": [
        {
            "HostDirectory": "/var/app/current/questions",
            "ContainerDirectory": "/usr/src/app/questions"
        }
    ],
    "Logging": "/var/log/nginx"
}